<% include partials/iframe_header %>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title">查询条件</h3>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-6 col-sm-8">
                            <span>地市：</span>
                            <input type="text" class=".col-xs-4" id="s_area" placeholder="请输入地市名称" />
                            <span>模板名：</span>
                            <input type="text" class=".col-xs-4" id="se_tname" placeholder="请输入模板名" />
                        </div>
                        <div class="col-md-3 col-sm-4">
                            <a class="btn btn-default btn-sm" href="javascript:void(0)" id="template-search"><i
                                    class="glyphicon glyphicon-search"></i>
                                查询</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="box" style="margin-bottom:auto">
                <div class="box-header with-border">
                    <b>工具栏： </b>
                    <caption>
                        <button type="button" class="btn btn-default" id="template_refresh"><i
                                class="fa fa-refresh icon-white"></i> 刷新
                        </button>

                    </caption>
                    <!--<h3 class="box-title">用户列表</h3>-->
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="users" class="table datatable table-bordered table-hover">
                        <thead>
                        <tr>
                            <th><input value="all" type="checkbox"
                                       onclick="$('.datatable tbody :checkbox').prop('checked', this.checked);"/>
                            </th>
                            <th>模板名</th>
                            <th>模板ID</th>
                            <th>地市名称</th>
                            <th>appkey</th>
                            <th>机器名</th>
                            <th>确认时间</th>
                           <th>操作</th>
                        </tr>
                        </thead>
                        <tbody>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->

<div class="modal fade" id="e-dialog-template" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="exampleModalLabel">用户修改</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" id="e-template-form">
                    <div class="box-body">
                        <input type="hidden" data-name="e_id" name="e_id" id="e_id"/>
                        <input type="hidden" data-name="e_opr" name="e_opr" id="e_opr"/>
                        <div class="form-group" >
                            <label for="name" class="col-sm-2 control-label">模板名</label>
                        <div class="col-sm-10">
 <input type="text" class="form-control" id="e_name" name="e_name"
                    >                                     
                    </div>
                        </div>
                        <div class="form-group" >
                            <label for="area" class="col-sm-2 control-label">地市</label>
                        <div class="col-sm-10">
 <input type="text" class="form-control" id="e_area" name="e_area"
                    readonly = "readonly" >                                     
                        </div>
                        </div>
                        <div class="form-group" id="form-template">
                            <label for="template" class="col-sm-2 control-label">模板样式</label>
                            <div class="col-sm-10">
                                <textarea class="form-control" rows="2" id="e_template_style" name="e_template_style"
                                       placeholder="请设置template" readonly = "readonly"></textarea><label id="template_password_desc"></label>
                            </div>
                        </div>
                        <div class="form-group" id="form-template">
                            <label for="template"  class="col-sm-2 control-label">模板位置</label>
                            <div class="col-sm-10">
                                <select class="selectpicker"  id="s_template_pos" name="s_template_pos" readonly = "readonly">
                                    <option value="1">上</option>
                                    <option value="2">下</option>
                                  </select>
                                <!-- <input type="text" class="form-control" id="e_template_pos" name="e_template_pos"
                                       placeholder="请输入template"> -->
                            </div>
                        </div>
                        <div class="form-group" id="form-starttime">
                            <label for="template" class="col-sm-2 control-label">启用时间</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control input-inline input-sm datetimepicker"  id="e_start_time" name="e_start_time"
                                       placeholder="请输入启用时间">
                            </div>
                        </div>
                        <div class="form-group" id="form-endtime">
                            <label for="template" class="col-sm-2 control-label">停用时间</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control input-inline input-sm form_datetime" id="e_end_time" name="e_end_time"
                                       placeholder="请输入停用时间">
                            </div>
                        </div>
                        <div class="form-group" id="form-status">
                            <label for="template" class="col-sm-2 control-label">状态</label>
                            <div class="col-sm-10">
                                  <select class="selectpicker"  id="s_status" name="s_status" >
                                    <option value="2" selected="selected">审核通过</option>
                                    <option value="3" >审核不通过</option>
                                  </select>

                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                <button type="button" class="btn btn-primary" id="saveTemplate">发布</button>
            </div>
        </div>
    </div>
</div>
<% include partials/iframe_footer %>

